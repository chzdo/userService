{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
     
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier":[
            "error",
            {
                "endOfLine":"auto",
                "trailingComma":"es5",
                "singleQuote":false,
                 "semi":true,
                 "useTabs":false,
                "usePrettierrc":false
            }
        ],
     
        "linebreak-style": [
            "error",
            "windows"
        ],
        "no-console":"error"
        

     
      
    }
}
